<p id="notice"><%= notice %></p>

<h1>Listing Store Adds</h1>

<table>
  <thead>
    <tr>
      <th>S</th>
      <th>Country</th>
      <th>State</th>
      <th>City</th>
      <th>Street</th>
      <th>Zipcode</th>
      <th>Timezone</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @store_adds.each do |store_add| %>
      <tr>
        <td><%= store_add.s_id %></td>
        <td><%= store_add.country %></td>
        <td><%= store_add.state %></td>
        <td><%= store_add.city %></td>
        <td><%= store_add.street %></td>
        <td><%= store_add.zipcode %></td>
        <td><%= store_add.timezone %></td>
        <td><%= link_to 'Show', store_add %></td>
        <td><%= link_to 'Edit', edit_store_add_path(store_add) %></td>
        <td><%= link_to 'Destroy', store_add, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Store add', new_store_add_path %>
